{
    "name": "@as-tral/cli",
    "version": "1.0.1",
    "type": "module",
    "files": [
        "/as-tral.d.ts",
        "/assembly",
        "/dist"
    ],
    "scripts": {
        "prepublish": "tsc",
        "asbuild:untouched": "asc assembly/index.ts --target debug",
        "asbuild:optimized": "asc assembly/index.ts --target release",
        "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
    },
    "bin": {
        "astral": "./dist/as-tral.js"
    },
    "dependencies": {
        "assemblyscript": "^0.20.4",
        "chalk": "^4.0.0",
        "escalade": "^3.1.1",
        "fast-glob": "^3.2.11",
        "sanitize-filename": "^1.6.3"
    },
    "devDependencies": {
        "@types/node": "^17.0.19",
        "typescript": "^4.5.5"
    }
}
